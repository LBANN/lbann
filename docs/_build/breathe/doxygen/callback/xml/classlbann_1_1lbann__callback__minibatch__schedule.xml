        <name>schedule</name>
        <reimplements refid="classlbann_1_1lbann__callback__variable__minibatch_1ace13e8941491ddd28ef35c277cecb1ad">schedule</reimplements>
        <param>
          <type>model *</type>
          <declname>m</declname>
        </param>
        <param>
          <type>int &amp;</type>
          <declname>new_mbsize</declname>
